{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "jsx": true,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es5",
    "loose": false,
    "externalHelpers": true,
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true,
      "optimizer": {
        "simplify": true
      },
      "react": {
        "runtime": "automatic",
        "importSource": "react"
      }
    }
  },
  "env": {
    "targets": [
      "defaults"
    ],
    "mode": "entry",
    "coreJs": 3
  },
  "sourceMaps": true,
  "minify": true // 是否开启压缩
}
